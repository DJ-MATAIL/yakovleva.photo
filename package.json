{
  "name": "yakovleva.photo",
  "version": "1.0.0",
  "description": "private project for yakovleva.photo website",
  "private": true,
  "scripts": {
    "build_client": "npx webpack --config webpack.config.js --mode development",
    "watch_client": "npx webpack --config webpack.config.js --mode development --watch",
    "build_production_client": "npx webpack --config webpack.config.js --mode production && npm run remove_webpack_odd_files",
    "remove_webpack_odd_files": "rm dist/preload.js",
    "start_server": "NODE_ENV=development node server/index.js",
    "start_production_server": "NODE_ENV=production node server/index.js",
    "build": "npm run build_production_client",
    "start": "npm run start_production_server"
  },
